
class Solution {
    public int findMaxK(int[] nums) {
        HashSet<Integer> set = new HashSet<>();
        int max = -1;

        for (int x : nums) {
            set.add(x);
        }

        if (set.size() == 0) return -1;  // direct check

        for (int x : nums) {
            if (x > 0 && set.contains(-x)) {
                max = Math.max(max, x);
            }
        }

        return max;
    }
}
