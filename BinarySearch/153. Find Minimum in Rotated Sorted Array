class Solution {
    public int findMin(int[] nums) {
        if(nums.length==1)  return nums[0];
        if(nums.length==2)  return Math.min(nums[0],nums[1]);
        int i=0;
        int j=nums.length-1;
        if(nums[i]<nums[j]) return nums[i];
        while(i<=j){
            int m=i+(j-i)/2;
            if(nums[m]>nums[m+1] && m<j) return nums[m+1];
            else if( m>i && nums[m-1]>nums[m]) return nums[m];
            else {if(nums[i]>nums[m]) j=m-1;
                       else i=m+1;
            }
        }
    return -1;
    }
    }
