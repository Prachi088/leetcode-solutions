class Solution {
    public int findGCD(int[] nums) {
        int max=nums[0];
        int min=nums[0];
   for(int i:nums){
    max=Math.max(max,i);
    min=Math.min(min,i);
   }
    if(max==min) return nums[0];
    return find(max, min);
    }
    public int find(int a,int b){
        if(a==0) {return b;}
         return find(b%a,a);
    }
}
